set term postscript color enh
set output 'cp1hall.eps'
set size 0.7
set ylabel '{/Symbol s}_{xy}'
set xlabel '{/Symbol b}_{1}-{/Symbol b}_{2}'
set key bottom right

pl 'cp1hall6' u ($12-0.2):($12*$34*pi/sin(pi/6.)*1.5) w lp title 'L=6, top surface' lt 1, \
 'cp1hall6' u ($12-0.2):(-$12*$37*pi/sin(pi/6.)*1.5) w lp title 'L=6, bottom surface' lt 1, \
 'cp1hall8' u ($12-0.2):($12*$34*pi/sin(pi/8.)*1.5) w lp title 'L=8, top surface' lt 2, \
 'cp1hall8' u ($12-0.2):(-$12*$37*pi/sin(pi/8.)*1.5) w lp title 'L=8, bottom surface' lt 2, \
 'cp1hall10' u ($12-0.2):($12*$34*pi/sin(pi/10.)*1.5) w lp title 'L=10, top surface' lt 3, \
 'cp1hall10' u ($12-0.2):(-$12*$37*pi/sin(pi/10.)*1.5) w lp title 'L=10, bottom surface' lt 3, \
 0.5 title ''
